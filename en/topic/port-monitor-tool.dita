<topic id="port-monitor-tool">
    <title>Port Monitor Tool</title>
    <shortdesc>This tool is used to debug a FXS port.</shortdesc>
    <body>
        <p><image href="../image/port-monitor-tool.jpg" width="650" id="image_dsc_rws_zgb"/></p>
        <?oxy_insert_start author="Yeastar" timestamp="20190722T090122+0800"?>
        <p>Go to <menucascade>
                <uicontrol>Status</uicontrol>
                <uicontrol>Reports</uicontrol>
                <uicontrol>Port Monitor Tool</uicontrol>
            </menucascade>.</p>
        <?oxy_insert_end?>
        <p>Select a FXS port:<ul id="ul_xw4_jws_zgb">
                <li>click <uicontrol>Start</uicontrol> to monitor the FXS port.</li>
